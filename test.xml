<?xml version="1.0" encoding="UTF-8"?>
<program language="SOL25" description="Simple SOL25 program">
  <class name="Main" parent="Object">
    <method selector="run">
      <block arity="0">
        <assign order="1">
          <var name="x"/>
          <expr>
            <literal class="Integer" value="42"/>
          </expr>
        </assign>
        <assign order="2">
          <var name="y"/>
          <expr>
            <send selector="asString">
              <expr>
                <var name="x"/>
              </expr>
            </send>
          </expr>
        </assign>
        <assign order="3">
          <var name="z"/>
          <expr>
            <send selector="print">
              <expr>
                <var name="y"/>
              </expr>
            </send>
          </expr>
        </assign>
      </block>
    </method>
  </class>
</program>